package Week2BooleansConditionalsLoops;

public class quiz2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  String name = "Tommy";

		  int age = 52;

		  if (name == "Tommy") {

		    System.out.println("Hi, Tommy!");

		  } else if (age == 52) {

		    System.out.println("52 years old.");

		  }

		  System.out.println("Out of conditional.");

		}

	}

}
